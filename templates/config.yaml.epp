apiVersion: kots.io/v1beta1
kind: ConfigValues
metadata:
  name: cd4pe
spec:
  values:
    hostname:
      value: <%= $target_fqdn %>
    analytics:
      value: "true"
    root_email:
      value: <%= $root_user %>
    root_password:
      value: <%= $root_password %>
    accept_eula:
      value: has_accepted_eula
